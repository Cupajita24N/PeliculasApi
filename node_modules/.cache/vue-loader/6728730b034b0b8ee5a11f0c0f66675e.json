ment.documentElement.offsetHeight;\r\n\r\n        if (bottomOfWindow) {\r\n          currentPage += 1;\r\n          this.fetchGenres((currentPage += 1));\r\n        }\r\n      };\r\n    },\r\n\r\n    next() {\r\n      currentPage += 1;\r\n      this.fetchGenres(currentPage);\r\n    },\r\n    previous() {\r\n      currentPage -= 1;\r\n      this.fetchGenres(currentPage);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style >\r\n</style>"]}]}