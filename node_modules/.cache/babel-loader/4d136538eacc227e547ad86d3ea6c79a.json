AACA,wBAAA;AACA,MAAA,OADA,qBACA;AACA,aAAA,UAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,OAHA;AAIA,MAAA,SAAA,EAAA;AAJA;AADA,GA9BA;AAsCA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,aAAA,qCAAA,KAAA,KAAA,CAAA,WAAA;AACA;AAHA,GAtCA;AA2CA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,OADA,EACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,KAAA,CAAA,GAAA,CACA,YACA,OADA,GAEA,0DAHA,CADA;;AAAA;AACA,gBAAA,QADA;AAMA,gBAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KARA;AAUA,IAAA,gBAVA,8BAUA;AACA,WAAA,QAAA,GAAA,KAAA,YAAA,EAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,KAbA;AAcA,IAAA,cAdA,4BAcA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,KAhBA;AAiBA,IAAA,OAjBA,qBAiBA;AACA,WAAA,IAAA,GAAA,KAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,SAAA,KAAA,IAAA;;AACA,UAAA,KAAA,KAAA,CAAA,EAAA,IAAA,KAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,iDAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,IAAA,CAAA,IAAA,CAAA,KAAA,KAAA,CAAA,EAAA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,WAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,IAAA,CAAA;AACA;AACA,KA1BA;AA2BA,IAAA,YA3BA,0BA2BA;AACA,UAAA,CAAA,KAAA,KAAA,CAAA,MAAA,EAAA;AACA,aACA,mCAAA,KAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,GADA;AAGA;AAhCA;AA3CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <v-container>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"3\">\r\n          <v-hover v-slot=\"{ hover }\" open-delay=\"200\">\r\n            <v-card :elevation=\"hover ? 16 : 2\" :class=\"{ 'on-hover': hover }\">\r\n              <router-link :to=\"`/movie/${movie.id}`\">\r\n                <v-img :src=\"posterPath\" alt=\"\" class=\"\" />\r\n              </router-link>\r\n            </v-card>\r\n          </v-hover>\r\n        </v-col>\r\n        <v-col cols=\"12\" sm=\"8\">\r\n          <h1 class=\"grey--text text-darken-3 mt-5\">{{ this.movie.title }}</h1>\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"2\">\r\n              <v-rating\r\n                :value=\"movie.vote_average / 2\"\r\n                color=\"amber\"\r\n                dense\r\n                half-increments\r\n                readonly\r\n                size=\"14\"\r\n              >\r\n              </v-rating>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"3\">\r\n              <span class=\"gray--text ml-n7\">\r\n                {{ Math.round(movie.vote_average * 10) }}% |\r\n                {{ movie.release_date }}\r\n              </span>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"7\">\r\n              <div class=\"subtitle-2 grey--text ml-n16\">\r\n                <span\r\n                  v-for=\"(item, index) in movie.genres\"\r\n                  :key=\"index\"\r\n                  class=\"ml-1\"\r\n                >\r\n                  {{ item.name }}\r\n                  <span v-if=\"movie.genres.length - 1 != index\">,</span>\r\n                </span>\r\n              </div>\r\n            </v-col>\r\n          </v-row>\r\n          <p class=\"mt-5 grey--text text--darken-3 subheader\">\r\n            {{ this.movie.overview }}\r\n          </p>\r\n          <div class=\"mt-5\">\r\n            <h2 class=\"mt-5 grey--text text--darken-3\">Reparto Destacado</h2>\r\n            <div\r\n              :key=\"index\"\r\n              v-for=\"(crew, index) in movie.credits.crew\"\r\n              class=\"mt-5\"\r\n            >\r\n              <div v-if=\"index < 2\" class=\"\">\r\n                <h3>{{ crew.name }}</h3>\r\n                <span class=\"grey--text\">{{ crew.job }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <v-dialog v-model=\"dialog\" persistent max-width=\"800px\">\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n              <v-btn\r\n                tile\r\n                color=\"primary\"\r\n                v-bind=\"attrs\"\r\n                v-on=\"on\"\r\n                @click.prevent=\"openYouTubeModel\"\r\n              >\r\n                <v-icon left>mdi-play</v-icon>Tr√°iler\r\n              </v-btn>\r\n            </template>\r\n            <v-card>\r\n              <v-card-title>\r\n                <span class=\"headline\">{{ this.movie.title }}</span>\r\n              </v-card-title>\r\n              <v-card-text>\r\n                <v-container>\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"\">\r\n                      <div class=\"iframe-container\">\r\n                        <img :src=\"mediaURL\" v-if=\"!isVideo\" />\r\n                        <iframe\r\n                          allowfullscreen\r\n                          v-if=\"isVideo\"\r\n                          :src=\"mediaURL\"\r\n                        ></iframe>\r\n                      </div>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-container>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"error\" text @click=\"dialog = flase\">Close</v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-btn tile color=\"primary\" class=\"ml-2\" @click=\"addFavo\">\r\n            <v-icon left>mdi-heart</v-icon>Favorito\r\n          </v-btn>\r\n        </v-col>\r\n      </v-row>\r\n      <v-divider class=\"mt-2\"></v-divider>\r\n      <Cast :casts=\"movie.credits.cast\" />\r\n      <v-divider class=\"mt-2\"></v-divider>\r\n      <Images :images=\"movie.images.backdrops\" />\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Cast from \"../components/Cast.vue\";\r\nimport Images from \"../components/Images.vue\";\r\nexport default {\r\n  components: {\r\n    Cast,\r\n    Images,\r\n  },\r\n  data() {\r\n    return {\r\n      arrFinal: [],\r\n      temp: [1],\r\n      movie: {\r\n        credits: {\r\n          crew: {},\r\n        },\r\n        images: {\r\n          backdrops: {},\r\n        },\r\n        id:{\r\n          \r\n        }\r\n      },\r\n      isVideo: false,\r\n      mediaURL: \"\",\r\n      dialog: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.arrFinal = JSON.parse(localStorage.getItem(\"favoritos\")) || [];\r\n    console.log(this.arrFinal);\r\n    this.fetchMovie(this.$route.params.id);\r\n  },\r\n  watch: {\r\n    \"$route.params.id\": {\r\n      handler() {\r\n        this.fetchMovie(this.$route.params.id);\r\n      },\r\n      immediate: true,\r\n    },\r\n  },\r\n  computed: {\r\n    posterPath() {\r\n      return \"https://image.tmdb.org/t/p/w500/\" + this.movie.poster_path;\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchMovie(movieId) {\r\n      const response = await this.$http.get(\r\n        \"/movie/\" +\r\n          movieId +\r\n          \"?append_to_response=credits,videos,images&language=es-US\"\r\n      );\r\n      this.movie = response.data;\r\n    },\r\n\r\n    openYouTubeModel() {\r\n      this.mediaURL = this.youtubeVideo();\r\n      this.isVideo = true;\r\n    },\r\n    openImageModel() {\r\n      this.isVideo = false;\r\n    },\r\n    addFavo() {\r\n      this.temp = this.arrFinal;\r\n      console.log(typeof this.temp);\r\n      if (this.movie.id == this.temp) {\r\n        alert(\"Ya tienes agragado esta pelicula como favorita!\");\r\n      } else {\r\n        this.temp.push(this.movie.id);\r\n        localStorage.setItem(\"favoritos\", JSON.stringify(this.temp));\r\n      }\r\n    },\r\n    youtubeVideo() {\r\n      if (!this.movie.videos) return;\r\n      return (\r\n        \"https://www.youtube.com/embed/\" + this.movie.videos.results[0].key\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.iframe-container {\r\n  overflow: hidden;\r\n  padding-top: 56.25%;\r\n  position: relative;\r\n}\r\n\r\n.iframe-container iframe {\r\n  border: 0;\r\n  height: 100%;\r\n  left: 0;\r\n  position: absolute;\r\n  top: 0;\r\n  width: 100%;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}