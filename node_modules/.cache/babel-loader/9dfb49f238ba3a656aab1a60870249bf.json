A;AACA,YAAA,cAAA,GACA,QAAA,CAAA,eAAA,CAAA,SAAA,GAAA,MAAA,CAAA,WAAA,KACA,QAAA,CAAA,eAAA,CAAA,YAFA;;AAIA,YAAA,cAAA,EAAA;AACA,UAAA,WAAA,IAAA,CAAA;;AACA,UAAA,MAAA,CAAA,WAAA,CAAA,WAAA,IAAA,CAAA;AACA;AACA,OATA;AAUA,KAxBA;AA0BA,IAAA,IA1BA,kBA0BA;AACA,MAAA,WAAA,IAAA,CAAA;AACA,WAAA,WAAA,CAAA,WAAA;AACA,KA7BA;AA8BA,IAAA,QA9BA,sBA8BA;AACA,MAAA,WAAA,IAAA,CAAA;AACA,WAAA,WAAA,CAAA,WAAA;AACA;AAjCA;AAfA,CAAA","sourcesContent":["<template>\r\n  <div class=\"mx-3\">\r\n      <h2 class=\"mt-2 grey--text\"> Popular Actors</h2>    \r\n      <v-container fluid>\r\n          <v-row>\r\n                <v-col cols=\"12\" sm=\"3\" :key=\"actor.id\" v-for=\"actor in this.actors\">\r\n                    <ActorCard :actor=\"actor\" />\r\n                    </v-col> \r\n                    <v-col cols=\"12\" class=\"d-flex justify-center mt-5\">\r\n                        <v-btn class=\"mx-2\" fab dark small color=\"error\" v-on:click.prevent=\"previous()\">\r\n                            <v-icon dark>\r\n                                fas fa-step-backward\r\n                            </v-icon>\r\n                        </v-btn>\r\n                        <v-btn class=\"mx-2\" fab dark small color=\"error\" v-on:click.prevent=\"next()\">\r\n                            <v-icon dark>\r\n                                fas fa-step-forward\r\n                            </v-icon>\r\n                        </v-btn>\r\n                    </v-col>\r\n          </v-row>\r\n      </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nlet currentPage = 1;\r\nimport ActorCard from \"../components/ActorCard.vue\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      actors: [],\r\n    };\r\n  },\r\n  components: {\r\n    ActorCard,\r\n  },\r\n\r\n  mounted() {\r\n    this.fetchActors(currentPage);\r\n    // this.scroll();\r\n  },\r\n\r\n  methods: {\r\n    async fetchActors(page) {\r\n      try {\r\n        const response = await this.$http.get(\r\n          \"https://api.themoviedb.org/3/person/popular?page=\" + page\r\n        );\r\n        this.actors = response.data.results;\r\n        // this.actors.push(...response.data.results);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n\r\n    scroll() {\r\n      window.onscroll = () => {\r\n        let bottomOfWindow =\r\n          document.documentElement.scrollTop + window.innerHeight ===\r\n          document.documentElement.offsetHeight;\r\n\r\n        if (bottomOfWindow) {\r\n          currentPage += 1;\r\n          this.fetchActors((currentPage += 1));\r\n        }\r\n      };\r\n    },\r\n\r\n    next() {\r\n      currentPage += 1;\r\n      this.fetchActors(currentPage);\r\n    },\r\n    previous() {\r\n      currentPage -= 1;\r\n      this.fetchActors(currentPage);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/components"}]}