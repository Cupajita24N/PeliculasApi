k :to=\"`/movie/${movie.id}`\">\r\n      <v-img :src=\"posterPath\" alt=\"\" class=\"\"></v-img>\r\n      </router-link>\r\n      <v-card-title class=\"subtitle-2\">{{movie.title}}</v-card-title>\r\n      <v-card-text>\r\n          <v-row align=\"center\" class=\"mx-0\">\r\n              <v-rating :value=\"movie.vote_average / 2\" color=\"amber\" dense half-increments readonly size=\"14\">\r\n              </v-rating>\r\n              <div class=\"grey--text ml-4\">\r\n                  {{movie.vote_average * 10}} % | {{movie.release_date}}\r\n              </div>\r\n          </v-row>\r\n          <div class=\"my-4 subtitle-2\">\r\n              <span v-for=\"(genre, index) in movie.genre_ids\" :key=\"genre\">\r\n                  {{genreTypeName(genre, index)}}\r\n              </span>\r\n          </div>\r\n      </v-card-text>\r\n      <button class=\"btn mx-4\" @click=\"toggleLike\">\r\n        <span v-text=\"'AÃ±adir a Favoritos'\">\r\n        </span>\r\n        <i class=\"far fa-heart\"></i>\r\n      </button>\r\n  </v-card>\r\n\r\n  </v-hover>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    movie: {\r\n      required: true,\r\n    },\r\n     genres: {\r\n      required: true,\r\n    }\r\n  },\r\n  computed: {\r\n    posterPath(){\r\n      return \"https://image.tmdb.org/t/p/w500/\" + this.movie.poster_path\r\n    }\r\n  },\r\n  methods: {\r\n    genreTypeName(genraId, index) {\r\n\r\n      for(const item of this.genres) {\r\n        if(item.id == genraId) {\r\n          if(this.movie.genre_ids.length - 1 == index){\r\n            return item.name ;\r\n          }else {\r\n            return item.name + \",\";\r\n          }\r\n          \r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}